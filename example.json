{
  "system commands": {
    "file operations": {
      "list files": "ls -l",
      "search files": "find . -name '*.txt'",
      "count lines": "wc -l *.txt",
      "show directory tree": "tree -L 2"
    },
    "process management": {
      "list processes": "ps aux",
      "top processes": "top -n 1",
      "memory usage": "free -h"
    },
    "network tools": {
      "check connectivity": "ping -c 4 google.com",
      "show open ports": "netstat -tuln"
    }
  },
  "user preferences": {
    "shell customization": {
      "show aliases": "alias",
      "list directories": "alias ll='ls -lh'",
      "go to parent": "alias ..='cd ..'"
    },
    "prompt styling": "export PS1='\\u@\\h:\\w\\$ '"
  },
  "editor settings": {
    "vim config": "cat ~/.vimrc",
    "nano config": "cat ~/.nanorc"
  },
  "development tools": {
    "version control": {
      "git status": "git status",
      "git log": "git log --oneline -n 5",
      "git branches": "git branch -v"
    },
    "build tools": {
      "compile cpp": "g++ -std=c++11 main.cpp -o program",
      "run python tests": "python -m unittest discover tests"
    },
    "containers": {
      "docker images": "docker images",
      "docker containers": "docker ps -a"
    }
  },
  "system information": {
    "hardware details": {
      "cpu info": "lscpu",
      "disk usage": "df -h",
      "ram info": "cat /proc/meminfo"
    },
    "os details": {
      "distribution": "cat /etc/os-release",
      "kernel version": "uname -r"
    }
  },
  "backup and archiving": {
    "create archive": "tar -czvf backup.tar.gz /path/to/files",
    "list archive contents": "tar -tvf backup.tar.gz"
  },
  "text processing": {
    "word count": "wc -w file.txt",
    "find replace": "sed 's/old/new/g' file.txt"
  }
}
